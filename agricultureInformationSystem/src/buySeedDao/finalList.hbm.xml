<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

        <class name="buySeedDao.finalList" table="finalList">
        <id name="id" column="id">
            <generator class="increment" />
        </id>
        <property name="seedName" column="seedName" />
        <property name="seedCount" column="seedCount" />
        <property name="materialName" column="materialName" />
        <property name="materialCount" column="materialCount" />
        <property name="materialPrice" column="materialPrice" />
    </class>
        
</hibernate-mapping>